{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run CRUDPlataforma-app",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/crudplataforma-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/treinamentol4eapp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/app",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/CRUDPlataforma/app\"}"
      },
      "envFile": "${workspaceFolder}/app/.env2",
      "preLaunchTask": "Build CRUDPlataforma"
    }
  ]
}